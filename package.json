{
  "name": "erxes-api",
  "version": "0.1.0",
  "description": "GraphQL API for erxes apps",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon src --exec babel-node",
    "test": "mocha src/data/tests --reporter spec --compilers js:babel-core/register",
    "build": "babel src --out-dir dist --ignore tests --copy-files"
  },
  "keywords": [
    "Javascript",
    "Node.js",
    "Express",
    "GraphQL",
    "Apollo"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.4.3",
    "graphql-subscriptions": "^0.2.2",
    "graphql-tools": "^0.8.4",
    "meteor-random": "0.0.3",
    "mongoose": "^4.7.4",
    "subscriptions-transport-ws": "^0.3.1",
    "underscore": "^1.8.3",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "faker": "^4.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
